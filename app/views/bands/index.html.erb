<h1>Bands#index</h1>
<p>Find me in app/views/bands/index.html.erb</p>

<div class="card-band-container">
<% @bands.each do |band| %>
    <div class="card-band">
      <div class="card-band-img" style="background-image: url(<%=cl_image_path band.photos.first.key%>)" >
      </div>
      <div class="card-band-content">
        <h1><%= link_to band.name, band_path(band.id) %></h1>
        <h2><%= band.style %></h2>
        <p><%= band.description %></p>
      </div>
    </div>
  <% end %>
</div>
